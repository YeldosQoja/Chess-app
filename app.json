{
  "expo": {
    "name": "Chess",
    "slug": "chess",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yeldos.kozha.chess"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.yeldos.kozha.chess"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/chess/pawn-white.png",
            "./assets/images/chess/pawn-black.png",
            "./assets/images/chess/rook-white.png",
            "./assets/images/chess/rook-black.png",
            "./assets/images/chess/bishop-white.png",
            "./assets/images/chess/bishop-black.png",
            "./assets/images/chess/knight-white.png",
            "./assets/images/chess/knight-black.png",
            "./assets/images/chess/queen-white.png",
            "./assets/images/chess/queen-black.png",
            "./assets/images/chess/king-white.png",
            "./assets/images/chess/king-black.png"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9dc1397d-9db9-4354-b7dd-93012a81cba6"
      }
    },
    "owner": "yeldos.kozha"
  }
}
